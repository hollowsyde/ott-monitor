.pytest_cache/
.coverage
.coverage.*
coverage.xml
htmlcov/
.tox/


# IDE / Editor folders
# ----------------------------------------------------------------
.idea/
.vscode/
*.sublime-project
*.sublime-workspace


# OS-specific files
# ----------------------------------------------------------------
.DS_Store
Thumbs.db